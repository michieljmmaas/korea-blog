{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prConcurrentLimit": 10,
  "prPriority": 5,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true,
      "prPriority": 10
    },
    {
      "matchUpdateTypes": ["major"],
      "prPriority": 0
    }
  ],
  "platformAutomerge": true
}
